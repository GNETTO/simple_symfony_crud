{% extends 'dash.html.twig' %}

{% block body %}
	<div class="w-75 m-auto bg-light p-5 mt-5">
		<h2 class="text-warning text-center">
			Tous les inscris
		</h2>
		<table class="table table-hover">
			<thead>
				<tr>
					<th scope="col">id</th>
					<th scope="col">nom</th>
					<th scope="col">prenom</th>
					<th scope="col">email</th>
				</tr>
			</thead>
			<tbody>
				{% for student in tous %}
					<tr class="table-active">
						<th scope="row">{{ student.id }}</th>
						<td>{{ student.nom}}</td>
						<td>{{ student.prenom }}</td>
						<td>{{ student.email }}</td>
					</tr>

				{% endfor %}
			</tbody>
		</div>

	{% endblock %}
